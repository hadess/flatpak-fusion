diff --git a/CMakeLists.txt b/CMakeLists.txt
index 784c2d0..d55014b 100644
--- a/CMakeLists.txt
+++ b/CMakeLists.txt
@@ -193,6 +193,7 @@ set(LIBRARY_OUTPUT_PATH ${dir} CACHE PATH "Build directory" FORCE)
 #-------------------------------------------------------------------------------
 # add each component
 
+add_subdirectory("data")
 add_subdirectory("external")
 add_subdirectory("es-core")
 add_subdirectory("es-app")
diff --git a/data/CMakeLists.txt b/data/CMakeLists.txt
new file mode 100644
index 0000000..f07ef8c
--- /dev/null
+++ b/data/CMakeLists.txt
@@ -0,0 +1,6 @@
+install( FILES EmulationStation.desktop DESTINATION ${DATADIR}/applications/ )
+
+# PNG icons generated with
+# convert es_icon.ico EmulationStation.png
+# for i in 16 24 32 48 64 96 128 256; do for j in EmulationStation-*.png; do file $j | grep "${i} x ${i}" && mkdir icons/${i}x${i}/ && mv $j icons/${i}x${i}/EmulationStation.png ; done ; done
+add_subdirectory("icons")
diff --git a/data/EmulationStation.desktop b/data/EmulationStation.desktop
new file mode 100644
index 0000000..717d8bc
--- /dev/null
+++ b/data/EmulationStation.desktop
@@ -0,0 +1,9 @@
+[Desktop Entry]
+Name=EmulationStation
+GenericName=Emulator Front-end
+Comment=Graphical front-end for emulators with controller navigation
+Exec=emulationstation
+Icon=emulationstation
+Terminal=false
+Type=Application
+Categories=Game;Emulator;
